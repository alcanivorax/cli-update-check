    "url": "https://github.com/alcanivorax/cli-update-check"
  },
  "author": "alcanivorax",
  "license": "MIT",
  "packageManager": "pnpm@10.27.0",
  "devDependencies": {
    "@types/node": "^25.0.8",
    "@types/semver": "^7.7.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "semver": "^7.7.3"
  }
}
